{
    "name": "@zolyn/charon",
    "version": "1.0.0",
    "description": "Utils",
    "keywords": [
        "utils"
    ],
    "homepage": "https://github.com/Zolyn/Charon#readme",
    "bugs": {
        "url": "https://github.com/Zolyn/Charon/issues"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Zolyn/Charon.git"
    },
    "license": "MIT",
    "author": "Yumeoto Zorin <zorin1477@gmail.com>",
    "main": "index.js",
    "files": [
        "dist"
    ],
    "scripts": {
        "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s && git add CHANGELOG.md",
        "ensure": "git-ensure --no-behind -c",
        "lint": "eslint \"./src/**/*.ts\"",
        "lint:fix": "nr lint --fix",
        "prepare": "husky install",
        "release": "nr changelog && nr ensure && bumpp --commit --tag --push"
    },
    "config": {
        "commitizen": {
            "path": "@commitlint/cz-commitlint"
        }
    },
    "devDependencies": {
        "@antfu/ni": "^0.16.2",
        "@commitlint/cli": "^17.0.2",
        "@commitlint/config-conventional": "^17.0.2",
        "@commitlint/cz-commitlint": "^17.0.0",
        "@typescript-eslint/eslint-plugin": "^5.27.0",
        "@typescript-eslint/parser": "^5.27.0",
        "bumpp": "^7.1.1",
        "commitizen": "^4.2.4",
        "conventional-changelog-cli": "^2.2.2",
        "conventional-github-releaser": "^3.1.5",
        "eslint": "^8.17.0",
        "eslint-config-alloy": "^4.5.1",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-prettier": "^4.0.0",
        "git-ensure": "^0.1.0",
        "husky": "^8.0.0",
        "lint-staged": "^13.0.0",
        "prettier": "^2.6.2",
        "sort-package-json": "^1.57.0",
        "tsup": "^6.0.1",
        "typescript": "^4.7.3"
    }
}
